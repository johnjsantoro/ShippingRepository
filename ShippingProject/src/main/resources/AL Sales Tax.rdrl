import com.ibm.cpo.projects.shippingproject.ShippingObject;
import com.ibm.cpo.projects.shippingproject.OrderInfo;

rule "AL Sales Tax"
	dialect "java"
	when
		OrderInfo( itemCode == "300" )
	then
		OrderInfo fact0 = new OrderInfo();
		fact0.setOrderPrice( new java.math.BigDecimal("300.45") );
		insert( fact0 );
end
